<h2><%= @list.name %></h2>
<ul>
<% @list.bookmarks.each do |bookmark|%>
<li>Movie:<%= bookmark.movie.title %></li>
<p>Comment:<%= bookmark.comment %></p>
<% end %>
</ul>
<div><%= link_to "Add Movie", new_list_bookmark_path(@list.id) %></div>
<div><%= link_to "Back to Genres", lists_path %></div>

<% if @list.photo.attached? %>
<%= cl_image_tag @list.photo.key, height: 300, width: 400, crop: :fill %>
<% end %>
